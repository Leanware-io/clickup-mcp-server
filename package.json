{
  "name": "clickup-mcp-server",
  "description": "Model Context Protocol server for the ClickUp API",
  "version": "1.0.0",
  "author": "Leanware-io",
  "license": "MIT",
  "type": "module",
  "bin": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "mcp",
    "clickup",
    "cline",
    "claude",
    "model context protocol"
  ],
  "scripts": {
